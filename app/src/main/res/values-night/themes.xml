    <style name="Theme.MaoMe" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/brown_main</item>
        <item name="colorPrimaryVariant">@color/brown_main</item>
        <item name="colorOnPrimary">@color/grey_bg</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/brown_main</item>
        <item name="colorSecondaryVariant">@color/brown_main</item>
        <item name="colorOnSecondary">@color/grey_bg</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>